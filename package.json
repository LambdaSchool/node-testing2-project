    "knex": "^2.3.0",
    "sqlite3": "^5.1.2"
  },
  "devDependencies": {
    "@types/jest": "^29.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.27.0",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "supertest": "^6.3.1"
  }
}
