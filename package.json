    "sqlite3": "^5.0.2"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.9.0",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "supertest": "^6.2.2"
