  "repository": {
    "type": "git",
    "url": "git+https://github.com/bloominstituteoftechnology/node-testing2-project.git"
  },
  "dependencies": {
    "@types/jest": "^29.5.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "knex": "^2.4.2",
    "knex-cleaner": "^1.3.1",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^8.41.0",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "supertest": "^6.3.3"
  }
}
